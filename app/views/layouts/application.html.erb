    <script src="../assets/TemplateData/UnityProgress.js"></script>
    <script src="../assets/Build/UnityLoader.js"></script>
    <script>
      var unityInstance = UnityLoader.instantiate("unityContainer", "../assets/Build/JStest01.json", {onProgress: UnityProgress});
